<Window x:Class="Yatp.Desktop.ChartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Yatp.Desktop"
        xmlns:vm="clr-namespace:Yatp.Desktop;assembly=Yatp.Desktop.Core"
        mc:Ignorable="d"
        Title="ChartWindow" Height="450" Width="800">
    <Grid>
        <Label Content="{Binding Product, FallbackValue='Product'}" HorizontalAlignment="Left" Margin="47,46,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.848,0.348"/>
        <Label Content="{Binding MdpsCount, FallbackValue='MdpsCount'}" HorizontalAlignment="Left" Margin="151,133,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.848,0.348"/>
        <Button Content="Button" Command="{Binding NewData}" HorizontalAlignment="Left" Margin="342,59,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
